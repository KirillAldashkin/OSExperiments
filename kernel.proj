<Project DefaultTargets="Build">
	<PropertyGroup>
		<Debug>true</Debug>
		<CFlags>-m32 -ffreestanding -Wall -Wextra -fno-builtin -fno-stack-protector -nostartfiles -nodefaultlibs</CFlags>
		<LinkFlags>-melf_i386 -Ttext 0x7e00</LinkFlags>
	</PropertyGroup>
	<ItemGroup>
		<BootAsm Include="src/boot/**/*.asm"/>
		<KernelC Include="src/kernel/**/*.c"/>
		<KernelAsm Include="src/kernel/**/*.asm"/>
	</ItemGroup>
	<Import Project="kernel.targets"/>
	<Import Project="kernel.proj.user"/>
</Project>